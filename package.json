{
  "name": "koa2-starter",
  "version": "0.1.0",
  "description": "Koa2 application generator.",
  "bin": {
    "koa2": "bin/koa2.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liuxing/koa2-starter.git"
  },
  "keywords": [
    "koa2",
    "koa2-generator",
    "koa2-starter"
  ],
  "author": "Liu Xing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liuxing/koa2-starter/issues"
  },
  "homepage": "https://github.com/liuxing/koa2-starter#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.15.1",
    "download-git-repo": "^1.0.2",
    "inquirer": "^6.0.0",
    "ora": "^2.1.0"
  },
  "devDependencies": {
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^1.0.0-rc.9",
    "lint-staged": "^7.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
